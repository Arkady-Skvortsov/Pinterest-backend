{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "es2017",
    "sourceMap": true,
    "paths": {
      "@users/*": ["src/users/*"],
      "@user-settings/*": ["src/user-settings/*"],
      "@search/*": ["src/search/*"],
      "@roles/*": ["src/roles/*"],
      "@chat/*": ["src/chat/*"],
      "@boards/": ["src/boards/*"],
      "@pins/*": ["src/pins/*"],
      "@notification/*": ["src/notification/*"],
      "@notes/*": ["src/notes/*"],
      "@boards/*": ["src/boards/*"],
      "@messages/*": ["src/messages/*"],
      "@jwt-token/*": ["src/jwt-token/*"],
      "@history/*": ["src/history/*"],
      "@comments/*": ["src/comments/*"],
      "@auth/*": ["src/auth/*"]
    },
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": true,
    "skipLibCheck": true,
    "strictNullChecks": false,
    "noImplicitAny": false,
    "strictBindCallApply": false,
    "forceConsistentCasingInFileNames": false,
    "noFallthroughCasesInSwitch": false
  }
}
